<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tarena.fh.mapper.LogInfoMapper">

    <!--查询所有的登入日志列表-->
    <select id="findAll" resultType="LogInfo">
        SELECT * FROM loginfo
    </select>

    <!--根据userId查询所有的登入日志列表-->
     <select id="findOne" resultType="LogInfo">
         SELECT * FROM logInfo WHERE user_id=#{userId}
     </select>
</mapper>